{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "feather",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_statement"
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "command_line"
        }
      ]
    },
    "comment": {
      "type": "PATTERN",
      "value": "~.*"
    },
    "command_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "clause"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "_token"
          }
        },
        {
          "type": "STRING",
          "value": "."
        }
      ]
    },
    "clause": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "safe"
        },
        {
          "type": "STRING",
          "value": "async"
        },
        {
          "type": "STRING",
          "value": "sensitive"
        },
        {
          "type": "STRING",
          "value": "(!)"
        },
        {
          "type": "STRING",
          "value": "elevated"
        }
      ]
    },
    "_token": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "control_keyword"
        },
        {
          "type": "SYMBOL",
          "name": "clause_keyword"
        },
        {
          "type": "SYMBOL",
          "name": "signature_keyword"
        },
        {
          "type": "SYMBOL",
          "name": "variable_scalar"
        },
        {
          "type": "SYMBOL",
          "name": "variable_container"
        },
        {
          "type": "SYMBOL",
          "name": "variable_projection"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "control_keyword": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "if"
        },
        {
          "type": "STRING",
          "value": "else"
        },
        {
          "type": "STRING",
          "value": "end"
        },
        {
          "type": "STRING",
          "value": "repeat"
        },
        {
          "type": "STRING",
          "value": "while"
        },
        {
          "type": "STRING",
          "value": "success"
        },
        {
          "type": "STRING",
          "value": "failure"
        },
        {
          "type": "STRING",
          "value": "increase"
        },
        {
          "type": "STRING",
          "value": "decrease"
        }
      ]
    },
    "clause_keyword": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "is"
        },
        {
          "type": "STRING",
          "value": "as"
        },
        {
          "type": "STRING",
          "value": "into"
        },
        {
          "type": "STRING",
          "value": "with"
        },
        {
          "type": "STRING",
          "value": "without"
        }
      ]
    },
    "signature_keyword": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "trace"
        },
        {
          "type": "STRING",
          "value": "elapsed"
        },
        {
          "type": "STRING",
          "value": "timeout"
        },
        {
          "type": "STRING",
          "value": "silent"
        },
        {
          "type": "STRING",
          "value": "on"
        }
      ]
    },
    "variable_scalar": {
      "type": "PATTERN",
      "value": "!?@[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "variable_container": {
      "type": "PATTERN",
      "value": "!?::[a-zA-Z_][a-zA-Z0-9_]*(?::[a-zA-Z_][a-zA-Z0-9_]*)?"
    },
    "variable_projection": {
      "type": "PATTERN",
      "value": "::[a-zA-Z_][a-zA-Z0-9_]*:[a-zA-Z_][a-zA-Z0-9_]+"
    },
    "string": {
      "type": "PATTERN",
      "value": "'[^']*'"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+(\\.\\d+)?"
    },
    "boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_-]*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}