{
  "name": "nekonomicon-syntax",
  "displayName": "nekonomicon spell Syntax",
  "description": "TextMate grammar for nekonomicon (.spell) scripting language.",
  "version": "0.0.4",
  "repository": { "type": "git", "url": "https://github.com/VOID-DIV-0/nekonomicon"},
  "publisher": "VOID DIV0",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "nekonomicon",
        "aliases": ["Nekonomicon", "nekonomicon", "Neko", "neko"],
        "extensions": [".spell"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nekonomicon",
        "scopeName": "source.nekonomicon",
        "path": "./syntaxes/nekonomicon.tmLanguage.json"
      }
    ]
  }
}
