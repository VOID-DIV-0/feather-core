{
  "name": "nekonomicon-syntax",
  "displayName": "nekonomicon spell Syntax",
  "description": "TextMate grammar for nekonomicon (.spell) scripting language.",
  "version": "0.0.18",
  "repository": {
    "type": "git",
    "url": "https://github.com/VOID-DIV-0/nekonomicon"
  },
  "publisher": "void-div0",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages",
    "Themes"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nekonomicon",
        "aliases": [
          "Nekonomicon",
          "nekonomicon",
          "Neko",
          "neko"
        ],
        "extensions": [
          ".spell"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nekonomicon",
        "scopeName": "source.nekonomicon",
        "path": "./syntaxes/nekonomicon.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "Nekonomicon Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/nekonomicon-dark.json"
      }
    ]
  }
}
