{
  "name": "nekonomicon-syntax",
  "displayName": "nekonomicon spell Syntax",
  "description": "TextMate grammar for nekonomicon (.spell) scripting language.",
  "version": "0.0.3",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "nekonomicon",
        "aliases": ["nekonomicon", "nekonomicon"],
        "extensions": [".spell"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nekonomicon",
        "scopeName": "source.nekonomicon",
        "path": "./syntaxes/nekonomicon.tmLanguage.json"
      }
    ]
  }
}
