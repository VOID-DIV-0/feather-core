{
  "name": "feather-syntax",
  "displayName": "Feather Sky Syntax",
  "description": "TextMate grammar for Feather (.sky) scripting language.",
  "version": "0.0.1",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "feather",
        "aliases": ["Feather", "feather"],
        "extensions": [".sky"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "feather",
        "scopeName": "source.feather",
        "path": "./syntaxes/feather.tmLanguage.json"
      }
    ]
  }
}
